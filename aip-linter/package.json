{
  "name": "aip-linter",
  "displayName": "AIP Linter",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "proto",
    "protocol buffer",
    "protobuf",
    "lint"
  ],
  "activationEvents": [
    "onLanguage:proto3",
    "onLanguage:proto"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "aip-linter.api-linter",
        "title": "Google API-linter"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.63.0",
    "@vscode/test-electron": "^2.0.3",
    "eslint": "^8.6.0",
    "glob": "^7.2.0",
    "mocha": "^9.1.3",
    "typescript": "^4.5.4"
  }
}

